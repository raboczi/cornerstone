# condpermadmin OSGi conditional permissions
( org.osgi.framework.AdminPermission "(id=*)" "metadata" )
( org.osgi.framework.PackagePermission "org.osgi.framework" "import" )
( org.osgi.framework.PackagePermission "org.osgi.service.condpermadmin" "import" )
( org.osgi.framework.ServicePermission "org.osgi.service.condpermadmin.ConditionalPermissionAdmin" "get" )
( org.osgi.framework.PackagePermission "org.slf4j" "import" )
